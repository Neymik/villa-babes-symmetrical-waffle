{
  "name": "Villa Babes",
  "description": "Seller Assistant",
  "version": "1.4.3",
  "manifest_version": 3,
  "update_url": "https://api.villababes.com/extension-update",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/villa-babes-logo.png"
  },
  "content_scripts": [
    {
      "js": [
        "initElements.js",
        "dist/js/chunk-vendors.1.min.js", 
        "dist/js/app.1.min.js"
      ],
      "css": [
        "dist/css/app.1.css"
      ],
      "matches": [
        "https://onlyfans.com/*"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "webRequest",
    "webNavigation",
    "contextMenus",
    "management"
  ],
  "web_accessible_resources": [
    {
      "resources": ["*"],
      "matches": ["<all_urls>"]
    }
  ]
}